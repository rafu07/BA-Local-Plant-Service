{
  "name": "plantservice",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/xb-msg-amqp-v100": "^0.9.58",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.102.17",
    "sqlite3": "^5.0.4"
  },
  "scripts": {
    "start": "cds run",
    "watch-plantserviceapp": "cds watch --open plantserviceapp/webapp/index.html?sap-ui-xx-viewCache=false#fe-lrop-v4"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": {
          "kind": "hana"
        },
        "auth": {
          "kind": "xsuaa"
        }
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "db.sqlite"
        }
      },
      "messaging": {
        "[hybrid]": {
          "kind": "enterprise-messaging-shared"
        }
      }
    }
  },
  "sapux": [
    "app/plantserviceapp"
  ]
}